{
	"apiVersion": "v1",
	"kind": "Pod",
	"metadata": {
		"name": "kube-addon-manager",
		"namespace": "kube-system",
		"version": "v1"
	},
	"spec": {
		"hostNetwork": true,
		"containers": [
			{
				"name": "kube-addon-manager",
				"image": "k8s.gcr.io/kube-addon-manager-amd64:v6.1",
				"resources": {
					"requests": {
						"cpu": "5m",
						"memory": "50Mi"
					}
				},
				"volumeMounts": [
					{
						"name": "addons",
						"mountPath": "/etc/kubernetes/addons",
						"readOnly": false
					}
				]
			}
		],
		"volumes": [
			{
				"name": "addons",
				"emptyDir": {}
			}
		]
	}
}